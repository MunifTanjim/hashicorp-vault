{
  "method": "post",
  "summary": "Destroy an AppRole secret ID.",
  "path": "/auth/approle/role/:role_name/secret-id/destroy",
  "parameters": [
    {
      "name": "role_name",
      "type": "string",
      "required": true,
      "description": "Name of the AppRole."
    },
    {
      "name": "secret_id",
      "type": "string",
      "required": true,
      "description": "Secret ID attached to the role."
    }
  ]
}
