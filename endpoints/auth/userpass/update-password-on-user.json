{
  "method": "post",
  "summary": "Update password for an existing user.",
  "path": "/auth/userpass/users/:username/password",
  "parameters": [
    {
      "name": "username",
      "type": "string",
      "required": true,
      "description": "The username for the user."
    },
    {
      "name": "password",
      "type": "string",
      "required": true,
      "description": "The password for the user."
    }
  ]
}
