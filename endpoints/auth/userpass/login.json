{
  "method": "post",
  "summary": "Login with the username and password.",
  "path": "/auth/userpass/login/:username",
  "parameters": [
    {
      "name": "username",
      "type": "string",
      "required": true,
      "description": "The username for the user."
    },
    {
      "name": "password",
      "type": "string",
      "required": true,
      "description": "The password for the user."
    }
  ]
}
