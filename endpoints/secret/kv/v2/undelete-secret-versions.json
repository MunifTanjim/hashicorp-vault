{
  "method": "post",
  "summary": "Undeletes the data for the provided version and path in the key-value store.",
  "path": "/:kvDir/undelete/:path",
  "parameters": [
    {
      "name": "kvDir",
      "type": "string",
      "description": "KV mount directory."
    },
    {
      "name": "path",
      "type": "string",
      "required": true,
      "description": "Specifies the path of the secret to undelete."
    },
    {
      "name": "versions",
      "type": "array",
      "items": {
        "type": "integer"
      },
      "required": true,
      "description": "The versions to undelete."
    }
  ]
}
