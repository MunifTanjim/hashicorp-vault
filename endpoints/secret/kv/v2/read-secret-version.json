{
  "method": "get",
  "summary": "This endpoint retrieves the secret at the specified location.",
  "path": "/:kvDir/data/:path",
  "parameters": [
    {
      "name": "kvDir",
      "type": "string",
      "description": "KV mount directory."
    },
    {
      "name": "path",
      "type": "string",
      "required": true,
      "description": "Specifies the path of the secret to read."
    },
    {
      "name": "version",
      "type": "integer",
      "required": false,
      "description": "Specifies the version to return."
    }
  ]
}
