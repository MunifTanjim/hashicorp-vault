{
  "userpass": {
    "createUser": {
      "method": "post",
      "params": {
        "bound_cidrs": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "max_ttl": {
          "type": "string"
        },
        "password": {
          "required": true,
          "type": "string"
        },
        "policies": {
          "type": "string"
        },
        "ttl": {
          "type": "string"
        },
        "username": {
          "required": true,
          "type": "string"
        }
      },
      "path": "/auth/userpass/users/:username"
    },
    "deleteUser": {
      "method": "delete",
      "params": {
        "username": {
          "required": true,
          "type": "string"
        }
      },
      "path": "/auth/userpass/users/:username"
    },
    "listUsers": {
      "method": "list",
      "path": "/auth/userpass/users"
    },
    "login": {
      "method": "post",
      "params": {
        "password": {
          "required": true,
          "type": "string"
        },
        "username": {
          "required": true,
          "type": "string"
        }
      },
      "path": "/auth/userpass/login/:username"
    },
    "readUser": {
      "method": "get",
      "params": {
        "username": {
          "required": true,
          "type": "string"
        }
      },
      "path": "/auth/userpass/users/:username"
    },
    "updatePasswordOnUser": {
      "method": "post",
      "params": {
        "password": {
          "required": true,
          "type": "string"
        },
        "username": {
          "required": true,
          "type": "string"
        }
      },
      "path": "/auth/userpass/users/:username/password"
    },
    "updatePoliciesOnUser": {
      "method": "post",
      "params": {
        "policies": {
          "type": "string"
        },
        "username": {
          "required": true,
          "type": "string"
        }
      },
      "path": "/auth/userpass/users/:username/policies"
    },
    "updateUser": {
      "method": "post",
      "params": {
        "bound_cidrs": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "max_ttl": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "policies": {
          "type": "string"
        },
        "ttl": {
          "type": "string"
        },
        "username": {
          "required": true,
          "type": "string"
        }
      },
      "path": "/auth/userpass/users/:username"
    }
  }
}